{
    "title": "Cloud Native Weekly - cloudnativego.com",
    "description": "Cloud Native weekly",
    "language": "en-us",
    "author": "cloudnativego.com",
    "links": {
        "sidebar": {
            "CloudNativeGo": "https://cloudnativego.com"
        }
    },
    "plugins": [
        "github",
        "codesnippet",
        "splitter",
        "page-toc-button",
        "editlink",
        "back-to-top-button",
        "-lunr", "-search", "search-plus",
        "github-buttons@2.1.0",
        "favicon@^0.0.2",
        "tbfed-pagefooter@^0.0.1",
        "3-ba",
        "theme-default",
        "-highlight", "prism", "prism-themes"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/CloudNativeInfra/cloudnativeweekly"
        },
        "editlink": {
            "base": "https://github.com/CloudNativeInfra/cloudnativeweekly",
            "label": "Edit this page"
        },
        "github-buttons": {
            "repo": "CloudNativeInfra/cloudnativeweekly",
            "types": ["star"],
            "size": "small"
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright Â© 2018 cloudnativego.com",
            "modify_label": " Updated at ",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "favicon": {
            "shortcut": "favicon.ico",
            "bookmark": "favicon.ico"
        },
        "3-ba": {
            "token": "c889b4cfd2df3f575151a52b18610b26"
        },
        "prism": {
            "css": [
            "prism-themes/themes/prism-ghcolors.css"
            ]
        }
    }
